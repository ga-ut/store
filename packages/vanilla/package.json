{
  "name": "@ga-ut/store-vanilla",
  "version": "0.0.2",
  "private": false,
  "type": "commonjs",
  "description": "Vanilla helpers for @ga-ut/store-core: watch/on/select/toObservable for framework-free usage.",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "keywords": [
    "vanilla",
    "watch",
    "observable",
    "state",
    "store",
    "state-management",
    "typescript"
  ],
  "homepage": "https://github.com/ga-ut/store",
  "repository": {
    "type": "git",
    "url": "https://github.com/ga-ut/store.git",
    "directory": "packages/vanilla"
  },
  "bugs": {
    "url": "https://github.com/ga-ut/store/issues"
  },
  "author": "ga-ut",
  "license": "MIT",
  "sideEffects": false,
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "dependencies": {
    "@ga-ut/store-core": "workspace:^"
  },
  "scripts": {
    "build": "node build",
    "test": "vitest run",
    "pub": "pnpm run build && pnpm publish --access public"
  }
}
