{
  "name": "@ga-ut/store-svelte",
  "version": "0.0.2",
  "private": false,
  "type": "commonjs",
  "description": "Svelte adapter for @ga-ut/store-core. Expose store slices as readable stores and bind actions easily.",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "keywords": [
    "svelte",
    "readable",
    "store",
    "state",
    "state-management",
    "bindMethods",
    "typescript"
  ],
  "homepage": "https://github.com/ga-ut/store",
  "repository": {
    "type": "git",
    "url": "https://github.com/ga-ut/store.git",
    "directory": "packages/svelte"
  },
  "bugs": {
    "url": "https://github.com/ga-ut/store/issues"
  },
  "author": "ga-ut",
  "license": "MIT",
  "sideEffects": false,
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "dependencies": {
    "@ga-ut/store-core": "workspace:^"
  },
  "peerDependencies": {
    "svelte": ">=4"
  },
  "scripts": {
    "build": "node build",
    "test": "vitest run",
    "pub": "pnpm run build && pnpm publish --access public"
  },
  "devDependencies": {
    "@sveltejs/vite-plugin-svelte": "^3.0.2",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/svelte": "^5.2.6",
    "@testing-library/user-event": "^14.5.2",
    "svelte": "^4.2.19"
  }
}
