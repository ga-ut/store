{
  "name": "@ga-ut/store-core",
  "version": "2.0.1",
  "private": false,
  "type": "commonjs",
  "description": "Tiny, type-safe state container with granular subscriptions and bound actions (framework-agnostic core).",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "keywords": [
    "state",
    "store",
    "state-management",
    "reactive",
    "observer",
    "pubsub",
    "proxy",
    "typescript"
  ],
  "homepage": "https://github.com/ga-ut/store",
  "repository": {
    "type": "git",
    "url": "https://github.com/ga-ut/store.git",
    "directory": "packages/core"
  },
  "bugs": {
    "url": "https://github.com/ga-ut/store/issues"
  },
  "author": "ga-ut",
  "license": "MIT",
  "sideEffects": false,
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "scripts": {
    "build": "node build",
    "pub": "npm run build && npm publish --access public"
  }
}
