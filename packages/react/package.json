{
  "name": "@ga-ut/store-react",
  "version": "2.0.0",
  "private": false,
  "type": "commonjs",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "peerDependencies": {
    "react": ">=18.0.0",
    "@types/react": ">=18.0.0"
  },
  "dependencies": {
    "@ga-ut/store-core": "workspace:^"
  },
  "scripts": {
    "build": "node build",
    "test": "vitest run",
    "pub": "pnpm run build && pnpm publish --access public"
  }
}
